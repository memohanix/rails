<h1>Add new Url</h1>
<%= form_for(@posts) do |f| %>
<ul>
	<% @posts.errors.full_messages.each do |msg| %>
	<li>
		<%= msg %>
	</li>
	<% end %>
</ul>
<p>
	<label for="name">Name</label>:
	<%= f.text_field :name %>
</p>
<p>
	<label for="user">User</label>:
	<%= f.text_field :user %>
</p>
<p>
	<%= submit_tag "Create" %>
	<% end %>
</p>